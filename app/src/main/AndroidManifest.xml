<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.allstudio.adstest">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AdsTest">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/app_id"/> <!--Sample APP ID-->
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BannerActivity"
            android:label="@string/banner_ads_example"/>
        <activity android:name=".InterstitialActivity"
            android:label="@string/interstitial_ads_example"/>
        <activity android:name=".RewardingActivity"
            android:label="@string/rewarded_ads_example"/>
        <activity android:name=".NativeActivity"
            android:label="@string/native_ads_example"/>

        <activity android:name=".BannerAMActivity"
            android:label="@string/m_banner_ads_example"/>
        <activity android:name=".InterstitialAMActivity"
            android:label="@string/m_interstitial_ads_example"/>
        <activity android:name=".RewardingAMActivity"
            android:label="@string/m_rewarded_ads_example"/>
        <activity android:name=".NativeAMActivity"
            android:label="@string/m_native_ads_example"/>
    </application>

</manifest>